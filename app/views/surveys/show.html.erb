<h1><%= @survey.name %></h1>
<ul>
  <% @questions.each do |question| %>
    <li>
      <h2><%= question.text %></h2>
      <table>
        <thead>
        <td>Respondent Name</td>
        <td>Email</td>
        <td>Response</td>
        </thead>
        <% question.answers.each do |answer| %>
          <tr>
            <td>
              <%= answer.name %>
            </td>
            <td>
              <%= answer.email %>
            </td>
            <td>
              <%= answer.text %>
            </td>
          </tr>
        <% end %>
      </table>
    </li>
  <% end %>
</ul>
